{
    "extends": "eslint:recommended",
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-console": "off",
        "comma-dangle": ["error", "never"],
        "one-var": "off",
        "space-infix-ops": ["error", {"int32Hint": false}]
    },
    "env": {
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    }
}
